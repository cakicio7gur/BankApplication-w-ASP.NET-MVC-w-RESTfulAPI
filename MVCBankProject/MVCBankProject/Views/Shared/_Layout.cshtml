<html lang="en">
<head>
    <!-- Required meta tags-->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="au theme template">
    <meta name="author" content="Hau Nguyen">
    <meta name="keywords" content="au theme template">
    <!-- Title Page-->
    <title>RedBank</title>
    <!-- Fontfaces CSS-->
    <link href="~/Content/css/font-face.css" rel="stylesheet" media="all">
    <link href="~/Scripts/vendor/font-awesome-4.7/css/font-awesome.min.css" rel="stylesheet" media="all">
    <link href="~/Scripts/vendor/font-awesome-5/css/fontawesome-all.min.css" rel="stylesheet" media="all">
    <link href="~/Scripts/vendor/mdi-font/css/material-design-iconic-font.min.css" rel="stylesheet" media="all">
    <!-- Bootstrap CSS-->
    <link href="~/Scripts/vendor/bootstrap-4.1/bootstrap.min.css" rel="stylesheet" media="all">
    <!-- Vendor CSS-->
    <link href="~/Scripts/vendor/animsition/animsition.min.css" rel="stylesheet" media="all">
    <link href="~/Scripts/vendor/bootstrap-progressbar/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet" media="all">
    <link href="~/Scripts/vendor/wow/animate.css" rel="stylesheet" media="all">
    <link href="~/Scripts/vendor/css-hamburgers/hamburgers.min.css" rel="stylesheet" media="all">
    <link href="~/Scripts/vendor/slick/slick.css" rel="stylesheet" media="all">
    <link href="~/Scripts/vendor/select2/select2.min.css" rel="stylesheet" media="all">
    <link href="~/Scripts/vendor/perfect-scrollbar/perfect-scrollbar.css" rel="stylesheet" media="all">
    <!-- Main CSS-->
    <link href="~/Content/css/theme.css" rel="stylesheet" media="all">
</head>
<body class="animsition">
    <!-- HEADER DESKTOP-->
    <header class="header-desktop3">
        <div class="section__content section__content--p35">
            <div class="header3-wrap">
                <div class="header__logo">
                    <a href="#">
                        <h2 style="color:#FFF">RedBank</h2>
                    </a>
                </div>
                <div class="header__navbar">
                    <ul class="list-unstyled">

                        <li>
                            <a href="#">
                                <span class="bot-line"></span>Bireysel
                            </a>
                        </li>
                        <li class="has-sub">
                            <a href="#">
                                <span class="bot-line"></span>Kurumsal
                            </a>
                        </li>
                        <li class="has-sub">
                            <a href="#">
                                <span class="bot-line"></span>Girişimci
                            </a>
                        </li>
                    </ul>
                </div>
                <div class="header__tool">
                    <div class="account-wrap">
                        <div class="account-item account-item--style2 clearfix js-item-menu">
                            <div class="content">
                                <a class="js-acc-btn" href="#">Bireysel / Kurumsal</a>
                            </div>
                            <div class="account-dropdown js-dropdown">
                                <div class="account-dropdown__body">
                                    <div class="account-dropdown__item">
                                        <a href="#">
                                            <i class="zmdi zmdi-account-box-o"></i>Giriş Yap
                                        </a>
                                    </div>
                                    <div class="account-dropdown__item">
                                        <a href="#">
                                            <i class="zmdi zmdi-account-add"></i>Üye Ol
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </header>
    @RenderBody()



    <script src="~/Scripts/vendor/jquery-3.2.1.min.js"></script>
    <!-- Bootstrap JS-->
    <script src="~/Scripts/vendor/bootstrap-4.1/popper.min.js"></script>
    <script src="~/Scripts/vendor/bootstrap-4.1/bootstrap.min.js"></script>
    <!-- Vendor JS       -->
    <script src="~/Scripts/vendor/slick/slick.min.js">
    </script>
    <script src="~/Scripts/vendor/wow/wow.min.js"></script>
    <script src="~/Scripts/vendor/animsition/animsition.min.js"></script>
    <script src="~/Scripts/vendor/bootstrap-progressbar/bootstrap-progressbar.min.js">
    </script>
    <script src="~/Scripts/vendor/counter-up/jquery.waypoints.min.js"></script>
    <script src="~/Scripts/vendor/counter-up/jquery.counterup.min.js">
    </script>
    <script src="~/Scripts/vendor/circle-progress/circle-progress.min.js"></script>
    <script src="~/Scripts/vendor/perfect-scrollbar/perfect-scrollbar.js"></script>
    <script src="~/Scripts/vendor/chartjs/Chart.bundle.min.js"></script>
    <script src="~/Scripts/vendor/select2/select2.min.js">
    </script>
    <!-- Main JS-->
    <script src="~/Scripts/js/main.js"></script>
</body>
</html>
